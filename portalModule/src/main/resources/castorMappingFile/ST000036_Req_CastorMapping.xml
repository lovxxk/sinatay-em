<?xml version="1.0" encoding="GBK"?>
<mapping>
	<description>部份领取试算</description>
	<class name="cn.com.sinosoft.portalModule.transport.transactionObject.TXInsurance" auto-complete="false">
		<map-to xml="TranData" />
		<field name="transExeDate" type="string"
			handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="TransrDate" location="BaseInfo" />
		</field>
		<field name="transExeTime" type="string"
			handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="TransrTime" location="BaseInfo" />
		</field>
		<field name="tellerNo" type="string"
			handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="TellerNo" location="BaseInfo" />
		</field>
		<field name="transRefGUID" type="string"
			handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="TransrNo" location="BaseInfo" />
		</field>
		<field name="saleChannel" type="string"
			handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="SaleChannel" location="BaseInfo" />
		</field>
		<field name="sellType" type="string"
			handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="SellType" location="BaseInfo" />
		</field>
		<field name="transType" type="string"
			handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="FunctionFlag" location="BaseInfo" />
		</field>
		<field name="source" type="string"
			handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="Source" location="BaseInfo" />
		</field>
		<field name="businessDatum" collection="arraylist"
			type="cn.com.sinosoft.portalModule.transport.sinatay.TranRequest">
			<bind-xml name="TranRequest" />
		</field>
	</class>

	<class name="cn.com.sinosoft.portalModule.transport.sinatay.TranRequest"
		auto-complete="false">
		<field name="lcCont"
			type="cn.com.sinosoft.portalModule.transport.sinatay.LCCont">
			<bind-xml name="LCCont" />
		</field>
	</class>
	<class name="cn.com.sinosoft.portalModule.transport.sinatay.LCCont"
		auto-complete="false">
		<field name="orderId" type="string" handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="OrderId" />
		</field>
		<field name="contNo" type="string" handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="ContNo" />
		</field>
		<field name="applyMoney" type="string" handler="cn.com.sinosoft.portalModule.interfacePortal.xml.convertMessage.castorMapping.StringFieldHandler">
			<bind-xml name="ApplyMoney" />
		</field>
	</class>
</mapping>