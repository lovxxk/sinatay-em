<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="loginUser" extends="struts-default" namespace="/loginUser">
		<action name="login" class="geUserAction" method="login">
			<result name="success">/index.jsp</result>
			<result name="error">/index.jsp</result>
			<result name="active">/index.jsp</result>
		</action>
		<action name="queryGeCodePage" class="geCodeAction" method="findAllGeCode">
			<result name="success">/pageTest/page.jsp</result>
		</action>
	</package>
</struts>
