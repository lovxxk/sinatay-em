<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="exception" extends="footTraceInterceptor" namespace="/exception">
		<action name="findException" class="exceptionAction" method="findException">
			<result name="success">
				/system/exception/search.jsp
			</result>
		</action>
		<action name="viewExceptionDetil" class="exceptionAction" method="viewExceptionDetil">
			<result name="success">
				/system/exception/exceptionView.jsp
			</result>
		</action>
		
	</package>
</struts>
